{
    "ecmafeatures":{
            "modules": true,
            "arrowFunctions": true
    },
    "rules": {
        "indent": [
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            2
        ],
        "no-undef": [
            2
        ],

        "eqeqeq": [2],
        "dot-notation": [2, {
            "allowKeywords": true
        }],
        "no-eval": 2,
        "no-useless-call": 0,
        "vars-on-top": 2,
        "curly": [2, "multi-line"],
        "default-case": 2,
        "no-dupe-args": 2,
        "no-extra-semi": 2,
        "no-unreachable": 2,
        "valid-jsdoc": 0,
        "no-unexpected-multiline": 0,
        "no-this-before-super": 0,
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "constructor-super": 0,
        "padded-blocks": [2, "never"],
        "brace-style": [2, "1tbs", {
            "allowSingleLine": true
        }],
        "no-shadow": 2,
        "no-var": 2,
        "comma-dangle": 2,
        "no-return-assign": 2,
        "no-multiple-empty-lines": 2,
        "no-unused-expressions": 2,
        "space-before-keywords": [2, "always"],
        "space-after-keywords": [2, "always"],
        "space-before-blocks": 2,
        "space-before-function-paren": [2, {
            "anonymous": "always",
            "named": "never"
        }],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-ops": 0,
        "spaced-comment": [2, "always", {
            "exceptions": ["-", "+"],
            "markers": ["=", "!"]
        }],
        "key-spacing": [2, {
            "beforeColon": false,
            "afterColon": true
        }],
        "prefer-const": 2,
        "comma-spacing": [2, {
            "before": false,
            "after": true
        }],
        "one-var": [2, "never"],
        "semi-spacing": [2, {
            "before": false,
            "after": true
        }],

    },
    "globals": {
        "angular": [
            1
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint"
}